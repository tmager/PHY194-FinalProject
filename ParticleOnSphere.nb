Cell[CellGroupData[{

Cell["Diffusion sweeps including overlap prevention", "Section",
 CellChangeTimes->{{3.639564861647098*^9, 3.639564866684442*^9}, {
  3.639565484503561*^9, 3.639565485775545*^9}, {3.639566364425893*^9, 
  3.639566376187251*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"diffusionSweep", "[", 
   RowBox[{"x_", ",", "r_", ",", "\[Delta]_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"xnew", "=", "x"}], ",", "xold", ",", "nrst", ",", 
      RowBox[{"success", "=", "0"}]}], "}"}], ",", " ", 
    RowBox[{"(*", 
     RowBox[{
     "should", " ", "randomize", " ", "order", " ", "of", " ", "indices"}], 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"xold", "=", 
         RowBox[{
         "xnew", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
         "xnew", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], "=", 
         RowBox[{"sphericalDiffuse", "[", 
          RowBox[{
           RowBox[{
           "xnew", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
           ",", "\[Delta]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Find", " ", "the", " ", "index", " ", "of", " ", "the", " ", 
          "nearest", " ", "particle"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"nrst", "=", 
         RowBox[{"Last", "[", 
          RowBox[{"Nearest", "[", 
           RowBox[{
            RowBox[{"MapIndexed", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"#", "\[Rule]", " ", 
                RowBox[{
                "#2", "\[LeftDoubleBracket]", "1", 
                 "\[RightDoubleBracket]"}]}], "&"}], ",", "xnew"}], "]"}], 
            ",", 
            RowBox[{
            "xnew", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
            ",", "2"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
          RowBox[{
          "If", " ", "the", " ", "move", " ", "causes", " ", "overlap"}], ",",
           " ", 
          RowBox[{"reject", " ", "it"}]}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Norm", "[", 
            RowBox[{
             RowBox[{
             "xnew", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
             "-", 
             RowBox[{
             "xnew", "\[LeftDoubleBracket]", "nrst", 
              "\[RightDoubleBracket]"}]}], "]"}], "<", 
           RowBox[{
            RowBox[{
            "r", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], "+", 
            RowBox[{
            "r", "\[LeftDoubleBracket]", "nrst", 
             "\[RightDoubleBracket]"}]}]}], ",", 
          RowBox[{
           RowBox[{
           "xnew", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
           "=", "xold"}], ",", 
          RowBox[{"success", "++"}]}], "]"}], ";"}], 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{
         "only", " ", "works", " ", "for", " ", "uniform", " ", "particle", 
          " ", "radii"}], ",", 
         RowBox[{
         "needs", " ", "a", " ", "more", " ", "advanced", " ", "check", " ", 
          "otherwise"}]}], "*)"}], "\[IndentingNewLine]", 
       "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", 
         RowBox[{"Length", "[", "x", "]"}]}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"{", 
       RowBox[{"xnew", ",", "success"}], "}"}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.639565554134742*^9, 3.639565627130847*^9}, {
  3.6395657504885893`*^9, 3.639565908276432*^9}, {3.6395659449928513`*^9, 
  3.6395660281246977`*^9}, {3.639566075472302*^9, 3.639566077819737*^9}, {
