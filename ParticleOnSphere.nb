Cell[CellGroupData[{

Cell["Diffusion on a sphere", "Section",
 CellChangeTimes->{{3.639564861647098*^9, 3.639564883263088*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"sphericalDiffuse", "[", 
   RowBox[{"x_", ",", "\[Delta]_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"dx", ",", "xnew"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "Move", " ", "the", " ", "particle", " ", "along", " ", "the", " ", 
      "tangent", " ", "plane", " ", "by", " ", "a", " ", "certain", " ", 
      "amount"}], " ", "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{"Step", " ", "1"}], ":", " ", 
      RowBox[{
      "generate", " ", "a", " ", "move", " ", "in", " ", "3", "D", " ", 
       "space"}]}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"dx", "=", 
      RowBox[{"RandomVariate", "[", 
       RowBox[{
        RowBox[{"NormalDistribution", "[", 
         RowBox[{"0", ",", "\[Delta]"}], "]"}], ",", "3"}], "]"}]}], ";", " ", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"We", " ", "need", " ", "a", " ", "normally"}], "-", 
        RowBox[{"distributed", " ", "point", " ", "in", " ", "3", "D"}]}], 
       ",", " ", 
       RowBox[{
       "not", " ", "one", " ", "randomly", " ", "distributed", " ", "in", " ",
         "each", " ", "axis"}]}], " ", "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"Step", " ", "2"}], ":", " ", 
        RowBox[{
        "Locally", " ", "the", " ", "surface", " ", "normal", " ", "of", " ", 
         "a", " ", "sphere", " ", "is", " ", "parallel", " ", "to", " ", 
         "x"}]}], ";", " ", 
       RowBox[{
       "so", " ", "subtract", " ", "the", " ", "component", " ", "of", " ", 
        "dx", " ", "that", " ", "is", " ", "parallel", " ", "to", " ", "x", 
        " ", "and", " ", "hence", " ", "the", " ", "surface", " ", 
        RowBox[{"normal", ".", " ", "The"}], " ", "move", " ", "is", " ", 
        "now", " ", "along", " ", "the", " ", "tangent", " ", "plane"}]}], 
      " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"dx", "=", 
      RowBox[{"dx", "-", " ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"dx", ".", "x"}], ")"}], " ", 
        RowBox[{"x", "/", 
         RowBox[{"(", 
          RowBox[{"x", ".", "x"}], ")"}]}]}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{"Step", " ", "3"}], ":", " ", "Move"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"xnew", "=", 
      RowBox[{"x", "+", "dx"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{"Step", " ", "4"}], ":", " ", 
       RowBox[{"Reproject", " ", "onto", " ", "sphere"}]}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"xnew", " ", "=", " ", 
      RowBox[{"xnew", "/", 
       RowBox[{"Sqrt", "[", 
        RowBox[{"xnew", ".", "xnew"}], "]"}]}]}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.639564894627219*^9, 3.639564910237041*^9}, {
  3.639564956392997*^9, 3.639565058837309*^9}, {3.639565113376729*^9, 
